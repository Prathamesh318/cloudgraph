# CloudGraph Frontend Environment Variables
# Copy this file to .env.development or .env.production and fill in your values

# API URL
VITE_API_URL=http://localhost:3001

# EmailJS Configuration (get these from https://emailjs.com)
VITE_EMAILJS_SERVICE_ID=your_service_id
VITE_EMAILJS_FEEDBACK_TEMPLATE_ID=your_feedback_template_id
VITE_EMAILJS_AUTOREPLY_TEMPLATE_ID=your_autoreply_template_id
VITE_EMAILJS_PUBLIC_KEY=your_public_key
